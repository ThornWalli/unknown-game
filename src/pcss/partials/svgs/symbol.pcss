.partial[data-partial*="svgs/symbol"] {
  position: relative;
  display: inline-block;
  width: 100%;
  height: auto;
  &:before {
    display: block;
    content: "";
    padding-top: 100%;
  }
  svg {
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    max-width: 100%;
    max-height: 100%;
  }
}
