.partial[data-partial="organisms/dialog/map-view"] {
  background: $color_black;

  .map-view {
    &__time {
      letter-spacing: 2px;
      color: $color_green;
      font-weight: 300;
      padding: 4px;
      font-size: 10px;
      text-align: center;
    }
  }

  canvas {
    display: block;
    width: 256px;
    height: 256px;
    cursor: pointer;
  }
}
