.partial[data-partial="organisms/app"],
.partial[data-partial^="organisms/app/"] {
  background: #000;

  .app {
    &__sidebar {
      @extend .scrollbar-style;

      position: absolute;
      left: 0;
      top: 0;
      width: auto;
      height: 100%;
      overflow-y: scroll;
      overflow-x: hidden;
    }

    &__container {
      cursor: pointer;
      background: #111;
    }
  }
}
