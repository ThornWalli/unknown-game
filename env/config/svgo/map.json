{
  "name": "svgo",
  "task": "gp-boilerplate-environment/lib/tasks/svgo",
  "config": {
    "subtasks": [{
      "name": "default",
      "config": "<%= root %>/env/config/svgo/config.default.js",
      "files": {
        "src": ["<%= root %>/src/assets/svg/**/*.svg", "!<%= root %>/src/assets/svg/symbols/**/*.svg"],
        "dest": "<%= destination %>/assets/svg"
      }
    },{
      "name": "svg-symbols",
      "config": "<%= root %>/env/config/svgo/config.svg-symbols.js",
      "files": {
        "src": ["<%= root %>/generated/svg-symbols/**/*.svg"],
        "dest": "<%= destination_root %>/generated/svg-symbols"
      }
    }],
    "watch": [{
      "src": ["<%= root %>/src/assets/svg/**/*.svg", "!<%= root %>/src/assets/svg-symbols/**/*.svg"],
      "tasks": ["default"]
    }]
  }
}
